{
  "name": "cordova-release-tools",
  "description": "Tools for releasing Apache Cordova packages",
  "version": "0.0.1",
  "author": "Jan Piotrowski @janpio",
  "bin": {
    "cort": "./bin/run"
  },
  "bugs": "https://github.com/janpio/cordova-release-tools/issues",
  "dependencies": {
    "@oclif/command": "^1.5.13",
    "@oclif/config": "^1.13.0",
    "@oclif/plugin-help": "^2.1.6",
    "github-url-from-git": "^1.5.0",
    "issue-regex": "^3.1.0",
    "read-pkg": "^5.1.1",
    "read-pkg-up": "^6.0.0",
    "terminal-link": "^1.3.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.0",
    "@oclif/test": "^1.2.4",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^8.0.2",
    "mocha": "^5.2.0",
    "nyc": "^13.3.0",
    "rimraf": "^2.6.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/janpio/cordova-release-tools",
  "keywords": [
    "oclif"
  ],
  "license": "Apache-2.0",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "cort",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "janpio/cordova-release-tools",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
